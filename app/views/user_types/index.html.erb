<h1>Listado de los Tipos de Usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th>Deleted</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_types.each do |user_type| %>
      <tr>
        <td><%= user_type.nombre %></td>
        <td><%= user_type.descripcion %></td>
        <td><%= user_type.deleted %></td>
        <td><%= link_to 'Mostrar', user_type %></td>
        <td><%= link_to 'Editar', edit_user_type_path(user_type) %></td>
        <td><%= link_to 'Eliminar', user_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Tipo de Usuario', new_user_type_path %>
